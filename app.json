{
  "expo": {
    "name": "Attendify",
    "slug": "attendance_mobile",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/new-logo.png",

    "scheme": "attendancemobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.ab_rar_6024.attendance_mobile",

      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#E6F4FE"
      }
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "extra": {
      "apiUrl": "https://attendance-system-mobile.onrender.com",
      "eas": {
        "projectId": "2cfddd94-5aa9-45a9-a75e-29f2d3df495c"
      }
    },

    "owner": "ab_rar_6024"
  }
}
